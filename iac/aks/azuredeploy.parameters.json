{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "aks-cluster-dev"
        },
        "location": {
            "value": "eastus2"
        },
        "dnsPrefix": {
            "value": "aks-cluster-dev-dns"
        },
        "kubernetesVersion": {
            "value": "1.22.6"
        },
        "networkPlugin": {
            "value": "kubenet"
        },
        "enableRBAC": {
            "value": true
        },
        "enablePrivateCluster": {
            "value": false
        },
        "enableHttpApplicationRouting": {
            "value": false
        },
        "enableAzurePolicy": {
            "value": false
        },
        "enableSecretStoreCSIDriver": {
            "value": false
        },
        "vmssNodePool": {
            "value": true
        },
        "tags": {
            "value": {
                "project": "devops-challenge"
            }
        }
    }
}